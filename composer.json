{
    "name": "simtabi/laravel-modulizer",
    "description": "A CLI tool for creating Laravel packages(modules) skeleton.",
    "license": "MIT",
    "authors": [
        {
            "name": "Imani Manyara",
            "email": "imani@simtabi.com",
            "homepage": "https://simtabi.com",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/simtabi/laravel-modulizer",
    "keywords": ["Laravel", "Laravel Modules", "Generator"],
    "require": {
        "illuminate/support": "8.x|9.x",
        "symfony/filesystem": "^5.0|^6.0",
        "symfony/finder": "^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Simtabi\\Modulizer\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Simtabi\\Modulizer\\Providers\\ModulizerServiceProvider"
            ],
            "aliases": {
                "Modulizer": "Simtabi\\Modulizer\\Facades\\ModulizerFacade"
            }
        }
    },
    "suggest": {
        "sensiolabs/security-checker": "Required to use the security check command"
    }
}
